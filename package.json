    "@sentry/replay": "^7.116.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "esbuild": "^0.25.11",
    "flexsearch": "^0.8.212",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "image-size": "2.0.2",
    "mdx-bundler": "^10.1.1",
    "next": "^15.5.7",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "preact": "^10.27.2",
    "react": "19.2.0",
