    "remark-math": "^6.0.0",
    "sharp": "^0.34.4",
    "unist-util-visit": "^5.0.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@next/swc-darwin-x64": "^16.0.10",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.15",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
