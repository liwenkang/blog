    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.15",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "@testing-library/react": {
      "react": "^19.0.0"
    },
    "@testing-library/dom": {
      "react": "^19.0.0"
