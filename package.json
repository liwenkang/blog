    "@types/express": "^5.0.6",
    "@types/image-size": "^0.7.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/mdast": "^4.0.4",
    "@types/node": "^25.0.1",
    "@types/postcss-load-config": "^2.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.1",
    "@types/unist": "^3.0.3",
    "cross-env": "^10.1.0",
    "dedent": "^1.7.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-mdx": "^3.6.2",
